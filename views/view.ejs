<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title></title>

    <% include partials/stylesheets %>
  </head>
  <body class="container">
    <% include partials/navbar %>
    <div class="jumbotron">
      <h1 class="display-3">Zootopia</h1>
      <p class="lead">View All the Animals</p>
      <% for(var i=0; i < data.length; i++) {%>
         <li id="<%= data[i]._id %>"><%= data[i].name + " " + data[i].species%> <button class="btn btn-danger deleteAnimal" type="button"> Delete </button></li>
      <% } %>
    </div>

    <% include partials/javascripts %>
  </body>
</html>
